{
  "name": "bun-utilities",
  "version": "0.1.1",
  "main": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.mjs"
    }
  },
  "napi": {
    "name": "bun-utilities",
    "triples": {
      "additional": [
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.10.1",
    "ava": "^4.3.0",
    "bun-types": "^0.1.2",
    "mitata": "^0.1.6"
  },
  "engines": {
    "node": ">= 10"
  },
  "keywords": [
    "bun",
    "utilities",
    "rust",
    "bindings",
    "napi"
  ],
  "files": [
    "lib/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "artifacts": "node scripts/moveArtifacts.mjs",
    "build": "napi build --platform --release lib",
    "build:debug": "napi build --platform lib",
    "move": "node scripts/moveFiles.mjs",
    "gpr": "node scripts/gpr.mjs",
    "prepublishOnly": "node scripts/moveFiles.mjs",
    "test:node": "ava tests/node/*",
    "test:bun": "bun wiptest",
    "version": "napi version"
  },
  "packageManager": "yarn@3.2.1"
}
